<Window x:Class="MineguideEPOCParser.GUIApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MineguideEPOCParser.GUIApp"
        mc:Ignorable="d"
        DataContext="{Binding RelativeSource={RelativeSource Self}}"
        Closing="Window_Closing"
        Title="Mineguide EPOC Parser" Height="618" Width="1000">
    <TabControl Margin="10">
        <TabItem Header="Medication Parser">
            <local:MedicationParserControl x:Name="MedicationParserCtrl" />
        </TabItem>
        <TabItem Header="Medication Mapper and Grouping to Group Mapper">
            <local:MedicationMapperGroupingControl />
        </TabItem>
        <TabItem Header="Medication Grouping To Mapper">
            <local:MedicationGroupingToMapperControl />
        </TabItem>
        <TabItem Header="File Encoding Converter">
            <local:FileEncodingConverterControl />
        </TabItem>
        <TabItem Header="Measurements Parser">
            <local:MeasurementsParserControl />
        </TabItem>
        <TabItem Header="Medication Manual Validator">
            <local:MedicationManualValidatorControl />
        </TabItem>
    </TabControl>
</Window>
